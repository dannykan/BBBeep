# API URL
NEXT_PUBLIC_API_URL=http://localhost:3001

# LINE Login
NEXT_PUBLIC_LINE_CHANNEL_ID=your-line-channel-id
NEXT_PUBLIC_LINE_CALLBACK_URL=http://localhost:3000/auth/line/callback

# 登入方式設定（逗號分隔）: phone, line
# 台灣版只用 LINE 登入
NEXT_PUBLIC_LOGIN_METHODS=line

# Google Maps API (for reverse geocoding)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Google Analytics (GA4)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
