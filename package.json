{
  "name": "bbbeeep",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "dev": "concurrently \"pnpm dev:api\" \"pnpm dev:web\"",
    "dev:web": "pnpm --filter @bbbeeep/web dev",
    "dev:api": "pnpm --filter @bbbeeep/api start:dev",
    "build": "pnpm build:api && pnpm build:web",
    "build:web": "pnpm --filter @bbbeeep/web build",
    "build:api": "pnpm --filter @bbbeeep/api build",
    "lint": "pnpm -r lint",
    "test": "pnpm --filter @bbbeeep/api test",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "prisma:generate": "pnpm --filter @bbbeeep/api prisma:generate",
    "prisma:migrate": "pnpm --filter @bbbeeep/api prisma:migrate",
    "prisma:studio": "pnpm --filter @bbbeeep/api prisma:studio",
    "postinstall": "pnpm prisma:generate"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "bcrypt",
      "esbuild",
      "sharp",
      "@nestjs/core",
      "workerd",
      "unrs-resolver"
    ]
  }
}
